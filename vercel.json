{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "dashboard.html", "use": "@vercel/static" },
    { "src": "reservation.html", "use": "@vercel/static" },
    { "src": "offer.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/dashboard", "dest": "/dashboard.html" },
    { "src": "/reservation", "dest": "/reservation.html" },
    { "src": "/offer", "dest": "/offer.html" },
    { "src": "/calendar.ics", "dest": "/api/calendar.ics.js" },
    { "src": "/styles.css", "dest": "/styles.css" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
